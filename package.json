{
  "name": "tresdoce-nestjs-toolkit-docs",
  "description": "Tresdoce NestJS Toolkit documentation",
  "version": "0.0.0",
  "author": {
    "name": "Maximiliano \"Mex\" Delgado",
    "email": "mdelgado@tresdoce.com.ar",
    "url": "https://rudemex.github.io/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tresdoce/tresdoce-nestjs-toolkit-docs.git"
  },
  "bugs": {
    "url": "https://github.com/tresdoce/tresdoce-nestjs-toolkit-docs/issues"
  },
  "homepage": "https://github.com/tresdoce/tresdoce-nestjs-toolkit-docs#readme",
  "private": true,
  "license": "MIT",
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "test": "exit 0",
    "prebuild": "rimraf dist",
    "build": "docusaurus build --out-dir ./dist",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy --out-dir ./dist",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve --dir ./dist",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "lint": "eslint \"**/*.{ts,tsx,js,jsx}\" --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,md,json}\"",
    "pre-commit": "lint-staged",
    "postinstall": "husky install",
    "release": "standard-version"
  },
  "prettier": {
    "semi": true,
    "tabWidth": 2,
    "singleQuote": true,
    "printWidth": 100,
    "trailingComma": "all"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{ts,json,md}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-rc.1",
    "@docusaurus/preset-classic": "2.0.0-rc.1",
    "@mdx-js/react": "^1.6.22",
    "@svgr/webpack": "^6.3.0",
    "clsx": "^1.1.1",
    "docusaurus-lunr-search": "^2.1.14",
    "file-loader": "^6.2.0",
    "mdx-mermaid": "^1.2.3",
    "mermaid": "^9.1.3",
    "prism-react-renderer": "^1.3.5",
    "raw-loader": "^4.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rxjs": "^7.5.3",
    "url-loader": "^4.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@docusaurus/module-type-aliases": "2.0.0-rc.1",
    "@tsconfig/docusaurus": "^1.0.5",
    "@types/node": "^18.0.6",
    "@types/react": "^17.0.47",
    "@types/react-helmet": "^6.1.5",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.5.1",
    "standard-version": "^9.3.2",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.6.4"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
